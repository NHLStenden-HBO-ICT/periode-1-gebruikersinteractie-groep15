<Window x:Class="Prototype_Game_Interaction.StartSpel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prototype_Game_Interaction"
        mc:Ignorable="d"
        Title="StartSpel" Height="450" Width="800"
         WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel>
            <Image Source="/img/teruggaan.jpg" Width="150" Height="40" HorizontalAlignment="Left"  Mouse.MouseUp="Image_MouseUp" Cursor="Hand"/>
            <Image Source="/img/instellingen.jpg" Width="150" Height="40" HorizontalAlignment="Left" Mouse.MouseUp="Image_MouseUp_1" Cursor="Hand"/>
        </StackPanel>
        
        <Image Source="/img/start.jpg"  Width="200" Height="50" HorizontalAlignment="Left" VerticalAlignment="Bottom" Mouse.MouseUp="Image_MouseUp_2" Cursor="Hand"/>
        
    </Grid>
</Window>

<Window x:Class="Prototype_Game_Interaction.StartSpel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prototype_Game_Interaction"
        mc:Ignorable="d"
        Title="StartSpel" Height="450" Width="800">
    <Grid>
        <Button  Content="Spelen" Click="SpelenClick" Height="25" HorizontalAlignment="Left" Name="SpeelButton" VerticalAlignment="Top"  Width="100" ></Button>
        <Button  Content="Beginscherm" Click="MainWindowClick" Height="25" HorizontalAlignment="Left" Name="MainButton" VerticalAlignment="Top" Width="100" Margin="0,30,0,0" ></Button>
        <Button  Content="Instellingen" Click="InstellingenClick" Height="25" HorizontalAlignment="Left" Name="InstellingenButton" VerticalAlignment="Top" Width="100" Margin="0,276,0,0" ></Button>
    </Grid>
</Window>
